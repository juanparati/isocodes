{
    "name": "juanparati/iso-codes",
    "description": "A PHP library that provides ISO codes, currencies, languages, timezones and additional geopolitical information",
    "keywords": [
        "laravel",
        "iso",
        "countries",
        "locales",
        "codes",
        "regions",
        "languages",
        "currencies",
        "timezones",
        "flags"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "illuminate/support": "^8.34.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\ISOCodes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\ISOCodes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juanparati\\ISOCodes\\Providers\\ISOCodesProvider"
            ]
        },
        "aliases": {
            "ISOCodes": "Juanparati\\ISOCodes\\Facades\\ISOCodesFacade"
        }
    }
}
